<mvc:View
	controllerName="workshop.wt.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page title="{i18n>homePageTitle}">
		<headerContent>
			<Button 
				icon="sap-icon://action" 
				tooltip="Open dialog"
				press="onOpenHelloDialog"/>
		</headerContent>
		<content>
			<mvc:XMLView viewName="workshop.wt.view.HelloPanel"/>
			<mvc:XMLView viewName="workshop.wt.view.InvoiceList"/>
		</content>
	</Page>
</mvc:View>